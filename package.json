{
	"name": "speedy-moq",
	"displayName": "Speedy Moq",
	"description": "Code generation to speed up testing in C# using the Moq library.",
	"publisher": "daveoftheparty",
	"author": "Dave Speer",
	"repository": {
		"type": "git",
		"url": "https://github.com/daveoftheparty/speedy-moq"
	},
	"bugs": {
		"url": "https://github.com/daveoftheparty/speedy-moq/issues"
	},
	"license": "MIT",
	"version": "0.1.1",
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"C#",
		"Moq",
		"Testing",
		"TDD",
		"Generation"
	],
	"icon": "docs/images/icon.png",
	"galleryBanner": {
		"color": "#252526",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"activationEvents": [
		"onLanguage:csharp"
	],
	"main": "./client/out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Log Settings",
			"properties": {
				"speedyMoq.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "verbose",
					"description": "Traces the communication between VS Code and Speedy Moq."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && npm install && npm install && cd .."
	},
	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	},
	"devDependencies": {
		"@types/mocha": "^8.2.2",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"eslint": "^7.26.0",
		"mocha": "^8.3.2",
		"typescript": "^4.3.5"
	}
}
